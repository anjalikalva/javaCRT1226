import java.util.*;
class Merge
{
public static void main(String[] args)
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
{
	a[i]=s.nextInt();
}
for(int i=0;i<n-1;i++)
{
	if(a[i]<a[i+1])
	{
             System.out.print(a[i]+" ");
	}
}
	System.out.print(a[n-1]);
}
}

C:\crt java>javac Merge.java

C:\crt java>java Merge
7
2 1 4 4 5 3 5
1 4 3 5
